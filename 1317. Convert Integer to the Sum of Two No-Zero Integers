// check all the digit that it contains 0 or not
// if that digit contains 0 increase a and decrease b 
// isValid goint to check it contain 0 or not

class Solution {
    public int[] getNoZeroIntegers(int n) {
        for (int a = 1; a < n; a++) {
            int b = n - a;
            if (isValid(a) && isValid(b)) {
                return new int[]{a, b};
            }
        }
        return new int[0];
    }

    private boolean isValid(int num) {
        while (num > 0) {
            if (num % 10 == 0) return false;
            num /= 10;
        }
        return true;
    }
}
