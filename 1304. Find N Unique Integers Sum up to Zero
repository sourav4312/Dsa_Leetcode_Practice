class Solution {
    public int[] sumZero(int n) {
        ArrayList<Integer> list = new ArrayList<>();
        if(n%2 != 0){
               n= n-1;
               list.add(0);
        }
            for(int i=0; i<n/2; i++) {
                list.add(i+1);
                list.add(-(i+1));
            }
        
        int ans[] = new int[list.size()];
        for(int i=0; i<list.size(); i++){
            ans[i] = list.get(i);
        }
        return ans;
    }
}
